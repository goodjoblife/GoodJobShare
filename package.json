    "airbnb-prop-types": "^2.15.0",
    "classnames": "^2.2.5",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "marked": "^0.3.6",
    "path": "^0.12.7",
