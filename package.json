{
  "name": "GoodJobShare",
  "version": "0.1.0",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dashboard -- node bin/server.js",
    "start-production": "cross-env NODE_ENV=production node bin/server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src scripts *.js",
    "lint-staged": "lint-staged",
    "testonly": "cross-env NODE_ENV=test jest",
    "testonly:watch": "npm run testonly -- --watch",
    "test": "npm run lint && npm run testonly",
    "test:cov": "npm run lint && npm run testonly -- --coverage",
    "build": "npm run clean && cross-env NODE_ENV=production webpack src/index.js --config webpack.config.prod.js"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.js": "eslint"
  },
  "jest": {
    "moduleNameMapper": {
      "common(.*)": "<rootDir>/src/components/common$1",
      "utils(.*)": "<rootDir>/src/utils$1"
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
